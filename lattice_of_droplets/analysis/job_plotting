for restlength in 2.0;do
    for R in 50.0;do
        for radiusB in 1.0;do
           for Np in 100;do
             for areafraction in 0.1 0.2 0.3 0.4;do
                for koninit in 100.0;do
                      for koffinit in 0.0000001 0.0001 0.01;do
                        for gammaA in 0.01 1.0;do
                           for gammapatch in 0.0001;do
                                 bash runjob_plotgeneration.sh $restlength $R $radiusB $Np $areafraction $koninit $koffinit $gammaA $gammapatch
                           done
                        done
                      done
                done
             done
            done
        done
    done
done
exit

